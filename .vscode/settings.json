{
    "cSpell.enabled": false,
    "files.associations": {
        "*.log.*": "log",
        "iosfwd": "cpp",
        "*.ipp": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "chrono": "cpp",
        "complex": "cpp",
        "functional": "cpp",
        "string_view": "cpp",
        "random": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
        "utility": "cpp",
        "valarray": "cpp",
        "unordered_map": "cpp",
        "fstream": "cpp",
        "ostream": "cpp",
        "sstream": "cpp",
        "bitset": "cpp",
        "set": "cpp",
        "future": "cpp",
        "shared_mutex": "cpp",
        "stop_token": "cpp",
        "streambuf": "cpp",
        "regex": "cpp",
        "tuple": "cpp",
        "condition_variable": "cpp"
    },
    "C_Cpp.errorSquiggles": "Disabled",
    "cmake.preferredGenerators": [
        "Unix Makefiles",
        "MinGW Makefiles"
    ],
    "cmake.configureSettings": {
        "INSTALL_DEPS": "ON",
        "SCREW_DEBUG": "OFF"
    },
    "markdownlint.config": {
        "MD024": false
    },
    "cmake.installPrefix": "${workspaceRoot}/stage",
    "grammarly.userWords": [
        "ArUco"
    ],
    "git.ignoreLimitWarning": true
}
